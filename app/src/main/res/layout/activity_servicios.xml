<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/principal"
    android:textAlignment="gravity"
    tools:context=".Servicios">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/background_main_login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ExtraText">

        <ImageView
            android:id="@+id/back_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:contentDescription="@string/back_btn"
            android:importantForAccessibility="no"
            android:padding="14dp"
            android:src="@drawable/back_btn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.048"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/Servicios_1"
            android:layout_width="146dp"
            android:layout_height="56dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="121dp"
            android:text="SERVICIOS"
            android:textColor="@color/negro"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/searchView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.5"
            tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap,TextSizeCheck" />

        <androidx.appcompat.widget.SearchView
            android:id="@+id/searchView"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:background="#1AFFFFFF"
            android:backgroundTint="#1AE1F5FE"
            android:longClickable="true"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.443"
            app:queryHint="Buscar"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="-16dp" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout"
            android:layout_width="331dp"
            android:layout_height="105dp"
            android:background="@drawable/round_back_white10_20"
            android:orientation="horizontal"
            android:padding="20dp"
            app:barrierAllowsGoneWidgets="true"
            app:chainUseRtl="true"
            app:flow_firstHorizontalStyle="spread_inside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.548"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="71dp"
                android:layout_height="53dp"
                android:contentDescription="@string/car"
                android:src="@mipmap/ic_launcher"
                app:barrierAllowsGoneWidgets="true"
                app:layout_constrainedWidth="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth="parent"
                app:layout_constraintWidth_default="percent"
                tools:ignore="ImageContrastCheck" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="73dp"
                android:layout_height="49dp"
                android:text="@string/Lavadero"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.058"
                app:layout_constraintStart_toEndOf="@+id/imageView"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.275"
                tools:ignore="MissingConstraints,TextSizeCheck" />

            <Button
                android:id="@+id/button1"
                android:layout_width="101dp"
                android:layout_height="54dp"
                android:layout_marginEnd="4dp"
                android:backgroundTint="@color/principal"
                android:text="@string/Solicitar_turno"
                android:textAlignment="center"
                android:textColor="@color/blanco"
                android:textColorHighlight="@color/principal"
                android:textColorLink="@color/principal"
                android:textCursorDrawable="@mipmap/fab"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.49"
                tools:ignore="HardcodedText,SmallSp,TextSizeCheck,DuplicateSpeakableTextCheck" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout1"
            android:layout_width="331dp"
            android:layout_height="105dp"
            android:background="@drawable/round_back_white10_20"
            android:orientation="horizontal"
            android:padding="20dp"
            app:barrierAllowsGoneWidgets="false"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.326"
            tools:ignore="MissingConstraints">


            <Button
                android:id="@+id/button3"
                android:layout_width="101dp"
                android:layout_height="54dp"
                android:layout_marginEnd="4dp"
                android:backgroundTint="@color/principal"
                android:text="@string/Solicitar_turno"
                android:textAlignment="center"
                android:textColor="@color/blanco"
                android:textColorHighlight="@color/principal"
                android:textColorLink="@color/principal"
                android:textCursorDrawable="@mipmap/fab"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.698"
                tools:ignore="HardcodedText,SmallSp,TextSizeCheck,DuplicateSpeakableTextCheck" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="100dp"
                android:layout_height="22dp"
                android:text="@string/Chapa_y_pintura"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/button3"
                app:layout_constraintHorizontal_bias="0.25"
                app:layout_constraintStart_toEndOf="@+id/imageView3"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.493"
                tools:ignore="MissingConstraints,TextSizeCheck" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="47dp"
                android:layout_height="49dp"
                android:contentDescription="@mipmap/ic_spay"
                android:src="@mipmap/ic_spay"
                app:layout_constrainedWidth="true"
                tools:ignore="ImageContrastCheck"
                tools:layout_editor_absoluteX="-12dp"
                tools:layout_editor_absoluteY="-6dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout3"
            android:layout_width="331dp"
            android:layout_height="105dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/round_back_white10_20"
            android:orientation="horizontal"
            android:padding="20dp"
            app:chainUseRtl="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="58dp"
                android:layout_height="52dp"
                android:contentDescription="@mipmap/ic_tire"
                android:src="@mipmap/ic_tire"
                app:layout_constrainedWidth="true"
                app:layout_constraintWidth="wrap_content"
                tools:ignore="ImageContrastCheck,MissingConstraints"
                tools:layout_editor_absoluteX="-13dp"
                tools:layout_editor_absoluteY="-5dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:text="@string/Neumaticos"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.355"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.507"
                tools:ignore="MissingConstraints,TextSizeCheck" />

            <Button
                android:id="@+id/button4"
                android:layout_width="101dp"
                android:layout_height="54dp"
                android:layout_marginStart="9dp"
                android:backgroundTint="@color/principal"
                android:text="@string/Solicitar_turno"
                android:textAlignment="center"
                android:textColor="@color/blanco"
                android:textColorHighlight="@color/principal"
                android:textColorLink="@color/principal"
                android:textCursorDrawable="@mipmap/fab"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.659"
                app:layout_constraintStart_toEndOf="@+id/textView7"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.49"
                tools:ignore="HardcodedText,SmallSp,TextSizeCheck" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout4"
            android:layout_width="331dp"
            android:layout_height="105dp"
            android:background="@drawable/round_back_white10_20"
            android:orientation="horizontal"
            android:padding="20dp"
            app:chainUseRtl="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.761"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="58dp"
                android:layout_height="52dp"
                android:contentDescription="@mipmap/ic_oil"
                android:src="@mipmap/ic_oil"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ImageContrastCheck" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="62dp"
                android:layout_height="55dp"
                android:text="@string/Cambio_de_Aceite"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.378"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.488"
                tools:ignore="MissingConstraints,TextSizeCheck" />

            <Button
                android:id="@+id/button5"
                android:layout_width="101dp"
                android:layout_height="54dp"
                android:layout_marginStart="9dp"
                android:backgroundTint="@color/principal"
                android:text="@string/Solicitar_turno"
                android:textAlignment="center"
                android:textColor="@color/blanco"
                android:textColorHighlight="@color/principal"
                android:textColorLink="@color/principal"
                android:textCursorDrawable="@mipmap/fab"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textView8"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="HardcodedText,SmallSp,TextSizeCheck" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>