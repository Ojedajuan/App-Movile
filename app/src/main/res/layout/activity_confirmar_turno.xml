        android:textColor="@color/negro"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_atras_turno"
        android:layout_width="104dp"
        android:layout_height="48dp"
        android:layout_marginStart="163dp"
        android:layout_marginTop="636dp"
        android:layout_marginEnd="160dp"
        android:layout_marginBottom="47dp"
        android:background="@drawable/background_dorado"
        android:text="@string/btn_atras_turno"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/onfirmar_turno4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="279dp"
        android:layout_marginTop="475dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="232dp"
        android:text="@string/confirmar_turno4"
        android:textColor="@color/secundario"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="204dp"
        android:layout_height="210dp"
        android:layout_marginStart="94dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="319dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/checkmark" />

</androidx.constraintlayout.widget.ConstraintLayout>